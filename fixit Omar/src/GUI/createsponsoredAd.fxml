<AnchorPane id="AnchorPane" prefHeight="469.0" prefWidth="609.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CreatesponsoredAdController">
