      <Button layoutX="434.0" layoutY="430.0" mnemonicParsing="false" style="-fx-background-color: white;" text="Cancel" />
      <Button id="butcreateAd" fx:id="butcreateAd" layoutX="507.0" layoutY="430.0" mnemonicParsing="false" onAction="#createSponsoredAd" style="-fx-background-color: #3b5998;" text="Confirm" />
