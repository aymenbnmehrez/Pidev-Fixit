      <Label layoutX="14.0" layoutY="50.0" text="Ad Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextArea fx:id="textAdName" layoutX="11.0" layoutY="74.0" prefHeight="38.0" prefWidth="548.0" />
      <Label layoutX="14.0" layoutY="125.0" prefHeight="17.0" prefWidth="92.0" text="Ad description">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <TextArea fx:id="textAdDescription" layoutX="14.0" layoutY="142.0" prefHeight="84.0" prefWidth="548.0" />
